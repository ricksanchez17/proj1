   uart2: serial@98c000 {
    compatible = "qcom,geni-debug-uart";
    reg = <0 0x0098c000 0 0x4000>;
    clock-names = "se";
    clocks = <&gcc 83>;
    pinctrl-names = "default";
    pinctrl-0 = <&qup_uart3_default_state>;
    interrupts = <0 604 4>;
    #address-cells = <1>;
    #size-cells = <0>;
    status = "disabled";
